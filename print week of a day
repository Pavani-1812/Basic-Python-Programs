day=int(input("enter your day:"))
if(day==1):
  print("day 1 is sunday")
elif(day==2):
  print("day 2 is monday")
elif(day==3):
  print("day 3 is tuesday")
elif(day==4):
  print("day 4 is wednesday")
elif(day==5):
  print("day 5 is thursday")
elif(day==6):
  print("day 6 is friday")
elif(day==7):
  print("day 7 is saturday")
else:
  print("invalid")
