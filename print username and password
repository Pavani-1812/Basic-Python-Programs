user="Pavani"
pwd=345
username=input("enter your username:")
if username!=user:
  print("invalid username")
username=input("enter your username:")
if username==user:
   print("correct username")
password=int(input("enter your password:"))
if (username==user and password==pwd):
  print("login success")
else:
  print("enter valid password")
